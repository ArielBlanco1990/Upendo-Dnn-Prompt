IF EXISTS (SELECT * FROM sys.objects WHERE type = N'P' AND OBJECT_ID = OBJECT_ID(N'{databaseOwner}[{objectQualifier}uvm_Prompt_GetThemesUsed]'))
   DROP PROCEDURE {databaseOwner}[{objectQualifier}uvm_Prompt_GetThemesUsed];
GO

